<?xml version="1.0" encoding="utf-8" standalone="no"?><data-map>

	<db-entity name="ARTIST">
		<db-attribute isMandatory="true" isPrimaryKey="true" name="ARTIST_ID" type="BIGINT"/>
		<db-attribute isMandatory="true" length="254" name="ARTIST_NAME" type="CHAR"/>
		<db-attribute name="DATE_OF_BIRTH" type="DATE"/>
	</db-entity>

	<db-entity name="ARTIST_EXHIBIT">
		<db-attribute isMandatory="true" isPrimaryKey="true" name="ARTIST_ID" type="BIGINT"/>
		<db-attribute isMandatory="true" isPrimaryKey="true" name="EXHIBIT_ID" type="INTEGER"/>
	</db-entity>

	<db-entity name="AWARD">
		<db-attribute isMandatory="true" name="ARTIST_ID" type="BIGINT"/>
		<db-attribute isMandatory="true" isPrimaryKey="true" name="AWARD_ID" type="BIGINT"/>
		<db-attribute length="255" name="NAME" type="VARCHAR"/>
	</db-entity>

	<db-entity name="EXHIBIT">
		<db-attribute isMandatory="true" name="CLOSING_DATE" type="TIMESTAMP"/>
		<db-attribute isMandatory="true" isPrimaryKey="true" name="EXHIBIT_ID" type="INTEGER"/>
		<db-attribute isMandatory="true" name="GALLERY_ID" type="INTEGER"/>
		<db-attribute isMandatory="true" name="OPENING_DATE" type="TIMESTAMP"/>
	</db-entity>


	<db-relationship name="artistAwardArray" source="ARTIST" target="AWARD" toMany="true">
		<db-attribute-pair source="ARTIST_ID" target="ARTIST_ID"/>
	</db-relationship>

	<db-relationship fk="true" name="toArtist" source="AWARD" target="ARTIST">
		<db-attribute-pair source="ARTIST_ID" target="ARTIST_ID"/>
	</db-relationship>

	<db-relationship name="artistExhibitArray" source="ARTIST" target="ARTIST_EXHIBIT" toMany="true">
		<db-attribute-pair source="ARTIST_ID" target="ARTIST_ID"/>
	</db-relationship>

	<db-relationship fk="true" name="toArtist" source="ARTIST_EXHIBIT" target="ARTIST">
		<db-attribute-pair source="ARTIST_ID" target="ARTIST_ID"/>
	</db-relationship>


	<db-relationship fk="true" name="toExhibit" source="ARTIST_EXHIBIT" target="EXHIBIT">
		<db-attribute-pair source="EXHIBIT_ID" target="EXHIBIT_ID"/>
	</db-relationship>

	<db-relationship name="artistExhibitArray" source="EXHIBIT" target="ARTIST_EXHIBIT" toMany="true">
		<db-attribute-pair source="EXHIBIT_ID" target="EXHIBIT_ID"/>
	</db-relationship>

</data-map>